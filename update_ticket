public function updateTicket(TicketM $ticket)
    {
        $url = sprintf(
            '/helpdesk/tickets/%d.json',
            $ticket->getDisplayId()
        );
        $data = $ticket->toJsonData();
        $response = json_decode(
            $this->restCall(
                $url,
                self::METHOD_PUT,
                $data
            )
        );
        return $ticket->setAll(
            $response->ticket
        );
    }
